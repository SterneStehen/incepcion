[server]
init_file = /etc/mysql/init.sql
[mysqld]
# Base directory of the MariaDB installation
basedir = /usr
# Data directory for the MariaDB databases
datadir = /var/lib/mysql
# Temporary directory for MariaDB
tmpdir = /tmp
# Path to the PID file
pid-file = /run/mysqld/mysqld.pid
# Socket file location
socket = /var/run/mysqld/mysqld.sock
# Run MariaDB as the root user (not recommended for production)
user = root
# Port number MariaDB listens on
port = 3306
# Bind to all network interfaces
bind-address = 0.0.0.0
# Allow networking
skip-networking = false
# Set the default character set to UTF-8 Unicode
character-set-server = utf8mb4
# Set the default collation
collation-server = utf8mb4_general_ci
# Number of days to keep binary logs before automatic removal
expire_logs_days = 7


